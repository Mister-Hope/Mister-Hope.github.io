    "@vuepress/plugin-cache": "2.0.0-rc.99",
    "@vuepress/plugin-docsearch": "2.0.0-rc.102",
    "@vuepress/plugin-feed": "2.0.0-rc.102",
    "@vuepress/plugin-pwa": "2.0.0-rc.102",
    "@vuepress/plugin-revealjs": "2.0.0-rc.102",
    "@waline/client": "3.5.7",
    "flowchart.ts": "3.0.1",
